<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tpBackOfficeBTF">
    <default-activity>tpaBackOffice</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="tpaBackOffice">
      <page>/jsff/tpaBackOffice.jsff</page>
    </view>
    <task-flow-return id="returnTOTPDashboard">
      <outcome>
        <name>returnTOTPDashboard</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>tpaBackOffice</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>back</from-outcome>
        <to-activity-id>returnTOTPDashboard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
