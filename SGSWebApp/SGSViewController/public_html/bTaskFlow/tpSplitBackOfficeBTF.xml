<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tpSplitBackOfficeBTF">
    <default-activity>tpSplitBackOffice</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="tpSplitBackOffice">
      <page>/jsff/tpSplitBackOffice.jsff</page>
    </view>
    <task-flow-return id="tpReturnSplitBackOffice">
      <outcome>
        <name>tpReturnSplitBackOffice</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>tpSplitBackOffice</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>returnSplitBO</from-outcome>
        <to-activity-id>tpReturnSplitBackOffice</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
