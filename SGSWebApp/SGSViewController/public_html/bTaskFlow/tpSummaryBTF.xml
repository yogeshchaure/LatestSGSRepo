<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tpSummaryBTF">
    <default-activity>tpDashBoard</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__2">
      <managed-bean-name>ValueChangeListener</managed-bean-name>
      <managed-bean-class>com.sgs.view.ValueChangeListener</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="tpDashBoard">
      <page>/jsff/tpSummary.jsff</page>
    </view>
    <task-flow-call id="tpBackOfficeBTF">
      <task-flow-reference>
        <document>/bTaskFlow/tpBackOfficeBTF.xml</document>
        <id>tpBackOfficeBTF</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="tpSplitBackOfficeBTF">
      <task-flow-reference>
        <document>/bTaskFlow/tpSplitBackOfficeBTF.xml</document>
        <id>tpSplitBackOfficeBTF</id>
      </task-flow-reference>
    </task-flow-call>
    <task-flow-call id="tpCostPlusModelBTF">
      <task-flow-reference>
        <document>/bTaskFlow/tpCostPlusModelBTF.xml</document>
        <id>tpCostPlusModelBTF</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__6">
      <from-activity-id>tpDashBoard</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>backoffice</from-outcome>
        <to-activity-id>tpBackOfficeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__1">
        <from-outcome>splitbo</from-outcome>
        <to-activity-id>tpSplitBackOfficeBTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>costplus</from-outcome>
        <to-activity-id>tpCostPlusModelBTF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
